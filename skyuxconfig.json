{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "addin-client",
  "mode": "easy",
  "compileMode": "aot",
  "appSettings": {
    "myLibrary": {
      "name": "SKY UX Add-in Client Library"
    }
  },
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "@blackbaud/sky-addin-client"
    ]
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
